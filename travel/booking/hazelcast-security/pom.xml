<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.hazelcast.platform.demos.travel.booking</groupId>
		<artifactId>booking-root</artifactId>
		<version>5.3</version>
	</parent>

	<artifactId>booking-hazelcast-security</artifactId>

	<name>Hazelcast Platform - Booking - Hazelcast Security</name>
	<description>Travel Booking - Hazelcast Security</description>

	<properties>
		<my.docker.image.name>hazelcast-initializer</my.docker.image.name>
		<main.basedir>${project.parent.parent.parent.basedir}</main.basedir>
	</properties>

	<dependencies>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>booking-common</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>

	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
	</build>
</project>
