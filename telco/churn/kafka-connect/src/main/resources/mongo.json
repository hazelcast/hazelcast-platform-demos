{
  "name": "debezium-connector-mongodb", 
  "config": {
    "connector.class": "io.debezium.connector.mongodb.MongoDbConnector"
    ,"_comment": "Format of hosts is replica set name from Dockerfile/hostname:port"
    ,"mongodb.hosts": "churn/mongo:27017"
    ,"_ignore_database.include.list": "admin"
    ,"_ignore_collection.include.list": "customer"
    ,"mongodb.name": "debezium-mongo"
    ,"mongodb.user": "@my.other.admin.user@"
    ,"mongodb.password": "@my.other.admin.password@"
  }
}
